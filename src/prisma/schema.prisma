generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://root:root@localhost:5452/ndi_foundation_issuer_db?schema=public"
}
  model Person {
  id        Int     @default(autoincrement()) @id
  dateOfBirth    DateTime @default(now())
  firstname      String
  lastname       String
  cid            String
}
